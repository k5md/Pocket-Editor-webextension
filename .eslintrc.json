{
  "extends": [
      "airbnb-base",
      "plugin:jest/recommended",
      "plugin:react/recommended"
  ],
  "plugins": [
      "jsx",
      "jest",
      "react"
  ],
  "env": {
      "browser": true,
      "webextensions": true,
      "jest": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "jsx/mark-used-vars": 2,
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
  },
  "parser": "babel-eslint"
}